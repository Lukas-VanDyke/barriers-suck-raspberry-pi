//****************************************************************//
//	 Unit positions on the upper half of the screen:	//
//			     <---------QA---------->			//
//			     <---------QB---------->			//
//			___<---------K------------>			//
// 				  B1	 B2	B3				//
//			___<---------P------------>			//
// 				  B4	        B5 				//
//					 B6					//
//****************************************************************//

.align

.global Pawn
Pawn:
	.int	50			// Unit X Resolution (width)
	.int	2			// Unit Y Resolution (height)
	.int	10			// Pawn hp
	.int	0			// Unit x position
	.int	288			// Unit y position
 	.int 	0			// Bullet width
 	.int 	0			// Bullet height
 	.int 	475			// Bullet x position
  	.int 	675			// Bullet y position
	.int 	1			// Direction
	.int	10			// Kill count

.global Knight
Knight:
	.int	30			// Unit X Resolution (width)
	.int	30			// Unit Y Resolution (height)
	.int	30			// Knight hp
	.int	0			// Unit x position
	.int	150			// Unit y position
 	.int 	0			// Bullet width
 	.int 	0			// Bullet height
 	.int 	475			// Bullet x position
  	.int 	675			// Bullet y position
	.int 	1			// Direction
	.int	5			// Kill count

.global QueenA
QueenA:				// Upper queen
	.int	21			// Unit X Resolution (width)
	.int	21			// Unit Y Resolution (height)
	.int	100			// QueenA hp
	.int	524			// Unit x position
	.int	60			// Unit y position
 	.int 	0			// Bullet width
 	.int 	0			// Bullet height
 	.int 	475			// Bullet x position
  	.int 	675			// Bullet y position
	.int 	0			// Direction

.global QueenB
QueenB:				// Lower queen
	.int	21			// Unit X Resolution (width)
	.int	21			// Unit Y Resolution (height)
	.int	100			// QueenB hp
	.int	524			// Unit x position
	.int	105			// Unit y position
 	.int 	0			// Bullet width
 	.int 	0			// Bullet height
 	.int 	475			// Bullet x position
  	.int 	675			// Bullet y position
	.int 	1			// Direction

.global Player
Player:
	.int	50			// Player X Resolution (width)
	.int	10			// Player Y Resolution (height)
	.int	50			// Player HP
	.int	475			// Player x position
	.int	675			// Player y position
 	.int 	0			// bullet width
	.int 	0			// bullet height
 	.int 	475			// bullet position
	.int 	675			// bullet position


Wall1:
	.int	14			// X Position
	.int	240			// Y Position
	.int	350			// Width
	.int	5			// Height
	.int	4			// HP

Wall2:
	.int	412			// X Position
	.int	240			// Y Position
	.int	200			// Width
	.int	5			// Height
	.int	4			// HP

Wall3:
	.int	660			// X Position
	.int	240			// Y Position
	.int	350			// Width
	.int	5			// Height
	.int	4			// HP

Wall4:
	.int	89			// X Position
	.int	325			// Y Position
	.int	400			// Width
	.int	5			// Height
	.int	4			// HP

Wall5:
	.int	535			// X Position
	.int	325			// Y Position
	.int	400			// Width
	.int	5			// Height
	.int	4			// HP

Wall6:
	.int	14			// X Position
	.int	345			// Y Position
	.int	1000			// Width
	.int	5			// Height
	.int	6			// HP

	
